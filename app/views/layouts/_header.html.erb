<header id="main_menu" class="header navbar-fixed">            
            <div class="main_menu_bg">
                <div class="container">
                    <div class="row">
                        <div class="nave_menu">
                            <nav class="navbar navbar-default" id="navmenu">
                                <div class="container-fluid">
                                    <!-- Brand and toggle get grouped for better mobile display -->
                                    <div class="navbar-header">
                                        <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1" aria-expanded="false">
                                            <span class="sr-only">Toggle navigation</span>
                                            <span class="icon-bar"></span>
                                            <span class="icon-bar"></span>
                                            <span class="icon-bar"></span>
                                        </button>
                                        <%= link_to root_path, class: "navbar-brand" do %>
                                        	<%= image_tag "logo.png", class: "logo" %>
                                        <% end %>
                                    </div>

                                    <!-- Collect the nav links, forms, and other content for toggling -->



                                    <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">

                                        <ul class="nav navbar-nav navbar-left">
                                            <li><%= link_to "Home", root_path %></li>
                                            <li class="hide"><%= link_to "Shops", commerces_path %></li>
                                            <li class="hide"><a href="#service">What we do?</a></li>
                                        </ul>
                                        <ul class="nav navbar-nav navbar-right">
                                            <% if !commerce_signed_in? && !user_signed_in? %>
                                                <li><%= link_to "Sign Up", register_index_path %></li>
                                                <li><%= link_to "Login", login_index_path %></li>
                                            <% else %>
                                                <% if user_signed_in? %>
                                                <li><%= link_to "edit account", edit_user_registration_path %></li>
                                                <li><%= link_to "Logout", destroy_user_session_path, method: :delete %></li> 
                                                <% end %>
                                                <% if commerce_signed_in? %>
                                                <li><%= link_to "edit commerce", edit_commerce_registration_path %></li>
                                                <li><%= link_to "Logout", destroy_commerce_session_path, method: :delete %></li> 
                                                <% end %>
                                            <% end %>
                                        </ul>
                                    </div>

                                </div>
                            </nav>
                        </div>  
                    </div>

                </div>

            </div>
</header> 


